<view class="body">

  <view class="name">{{title}}</view>

  <view class="outline">
    <view class="inline" style="width:{{value}}; background:{{backColor}}">
      <text class="inline-text" wx:if="{{textPosition === 'inline'}}">{{value}}</text>
      <text class="outline-text">{{number}}</text>

    </view>
  </view>


</view>
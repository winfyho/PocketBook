/* pages/account/components/analyse/analyse.wxss */
.analyse-wraper{
    width: 100%;
    height: 130rpx;
    display: flex;
    box-sizing: border-box;
    padding: 0 30rpx;
    padding-top:20rpx;
}
.analyse-wraper .total{
    text-align: left;
}
.analyse-wraper .max{
    text-align: center;
}
.analyse-wraper .avg{
    text-align: right;
}
.analyse-wraper>view{
    flex: 1 1 auto;
}
.analyse-wraper>view .title{
    height: 40rpx;
    line-height: 40rpx;
    color: #888;
    font-size: 26rpx;

}
.analyse-wraper>view .number{
    height: 70rpx;
    line-height: 70rpx;
    font-size: 35rpx;
    color: #86BAA3;

}
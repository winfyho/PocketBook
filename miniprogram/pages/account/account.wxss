/* pages/account/account.wxss */
@import "/assets/css/base.wxss";
@import "./style/footer.wxss";
@import "./style/addRecord.wxss";
page{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 1;
  overflow: hidden;
}
.body{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  width: 100%;
  height: 100%;
  background: #fff;
}


.reach-bottom{
  position: absolute;
  bottom: -80rpx;
  
  width: 100%;
  height: 80rpx;
}
.reach-bottom>view{
  width: 100%;
  height: 100%;
  line-height: 80rpx;
  text-align: center;
}